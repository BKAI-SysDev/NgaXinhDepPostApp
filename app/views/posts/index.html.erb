<%= link_to "New post", posts_new_path %>
<% @posts.each do |post| %> 
  <div>
    <%= post.content %>
    <%= link_to 'Show this post', post_path(post) %>
    <%= link_to 'Edit', edit_post_path(post) %>
    <%= link_to 'Delete', delete_post_path(post), method: :delete %> 
  </div>
<% end %>
<%= will_paginate @posts %>
